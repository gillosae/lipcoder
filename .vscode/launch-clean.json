{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension (Clean)",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-extensions"
			],
			"outFiles": [
				"${workspaceFolder}/dist/client/**/*.js"
			],
			"skipFiles": [
				"<node_internals>/**"
			]
		}
	]
}
